# Copy this file to backend/.env and fill in production/development values (DO NOT commit real secrets)
DATABASE_URL=postgresql://<db_user>:<db_password>@localhost/skillsync
SECRET_KEY=replace-with-generated-secret
FLASK_ENV=development
FLASK_DEBUG=True
# path to Firebase service account (set on dev machine; don't commit)
FIREBASE_SERVICE_ACCOUNT=/path/to/firebase_credentials
